package com.br.cienciascomputacao.faj.Cafe.model;

public class Cafe {
	private int id;
	private String marca;
	private double peso;
	private String torracao;
	
	public Cafe(int id, String marca, double peso, String torracao) {
		this.id = id;
		this.marca = marca;
		this.peso = peso;
		this.torracao = torracao;
	}
	
	public Cafe() {
		
	}
	


}
